header.masthead__wrap
  .masthead
    .icon-to-logo
      img(src="assets/images/logo/logo@3x.png")

    nav
      ul.toggle
        li
          input#all.all(type='radio', name='item_type')
          label(for='all') all.
        li
          input#thought.thought-filter(type='radio', name='item_type')
          label(for='thought') thought.
        li
          input#creative.creative-filter(type='radio', name='item_type')
          label(for='creative') creative.
        li
          input#work.work-filter(type='radio', name='item_type')
          label(for='work') work.
        li
          a(href='#') sayhello.
    .social-icons
      div
        a.icon-behance
      div
        a.icon-google-plus
      div
        a.icon-facebook
      div
        a.icon-twitter
      div
        a.icon-github


